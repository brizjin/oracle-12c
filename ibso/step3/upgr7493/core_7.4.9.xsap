				<sql><![CDATA[cnt integer := 0;
	function extract_profile(p_props in varchar2) return varchar2 is
		i pls_integer;
		j pls_integer;
	begin
		i := instr(p_props, '|PROFILE ');
		if i > 0 then
			j := instr(p_props, '|', i + 1);
			if j > 0 then
				return nvl(upper(trim(substr(p_props, i+9, j-i-9))), 'DEFAULT');
			end if;
			return nvl(upper(trim(substr(p_props, i+9))), 'DEFAULT');
		end if;
		return 'DEFAULT';
	end;

	function get_profile_value(p_profile in varchar2, p_name in varchar2) return varchar2 is
		s varchar2(2000);
	begin
		select po(po.[value]: po_val) in profiles%rowtype
		where po.[profile] = p_profile and po.[resource_name] = p_name
		into s;
		return s;
	exception when no_data_found then
		if p_profile <> 'DEFAULT' then
			return get_profile_value('DEFAULT', p_name);
		else
  			return null;
		end if;
	end;

begin
	for (select u(u.[properties]: props)
			in users%rowtype
			where u.[TYPE]='U'
			and (u.[lock_status] is null or u.[lock_status] not in ('DELETED', 'LOCKED')))
	loop
		if nvl(get_profile_value(extract_profile(u.props), 'OOXML_USER'), 'NO') = 'YES' then
			cnt := cnt + 1;
		end if;
	end loop;
	return cnt;
end;]]></sql>
